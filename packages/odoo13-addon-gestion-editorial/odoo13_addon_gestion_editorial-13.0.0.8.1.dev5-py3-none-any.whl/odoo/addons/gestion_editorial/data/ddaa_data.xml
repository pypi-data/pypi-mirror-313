<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        <record id="product_category_ddaa" model="product.category">
            <field name="name">Derechos de autoría</field>
            <field name="parent_id">1</field>
        </record>
        <record id="stock_location_authors" model="stock.location">
            <field name="name">Autores</field>
            <field name="location_id" ref="stock.stock_location_locations_partner"/>
            <field name="usage">supplier</field>
            <field name="company_id"></field>
        </record>
        <record id="stock_picking_type_entrega_autoria" model="stock.picking.type">
            <field name="name">Entrega de libros a autoría</field>
            <field name="sequence_code">LIB_AUTOR</field>
            <field name="code">outgoing</field>
            <field name="show_reserved">True</field>
            <field name="show_operations">False</field>
            <field name="use_existing_lots">False</field>
            <field name="warehouse_id">1</field>
            <field name="default_location_src_id" ref="stock.stock_location_stock"/>
            <field name="default_location_dest_id" ref="stock_location_authors"/>
        </record>
    </data>
</odoo>
