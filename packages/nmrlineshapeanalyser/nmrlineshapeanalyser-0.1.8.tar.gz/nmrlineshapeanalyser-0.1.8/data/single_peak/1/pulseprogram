# 1 "C:/Bruker/TOPSPIN21/exp/stan/nmr/lists/pp/onepulse.av"
;onepulse.av (TopSpin 2.0)

;single pulse excitation, acquisition without decoupling

;parameters:
;d1 : recycle delay
;p1 : excitation pulse length
;pl1 : power level for excitation pulse

;$COMMENT=single pulse excitation, acquisition without decoupling
;$CLASS=Solids
;$DIM=1D
;$TYPE=direct excitation
;$SUBTYPE=simple 1D
;$OWNER=Bruker
# 1 "mc_line 16 file C:/Bruker/TOPSPIN21/exp/stan/nmr/lists/pp/onepulse.av dc-measurement inserted automatically"
    dccorr
# 16 "C:/Bruker/TOPSPIN21/exp/stan/nmr/lists/pp/onepulse.av"
1 ze
2 d1 pl1:f1
  (p1 ph1):f1
  go=2 ph31
  wr #0
exit

ph1= 0 2 1 3
ph31=0 2 1 3

