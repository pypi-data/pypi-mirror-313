    "FilePrompt": {
      "description": "Prompt loaded from a file.\n\nThis type of prompt loads its content from a file, allowing for longer or more\ncomplex prompts to be managed in separate files. The file content is loaded\nand parsed according to the specified format.",
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "description": {
          "title": "Description",
          "type": "string"
        },
        "arguments": {
          "items": {
            "$ref": "#/$defs/ExtendedPromptArgument"
          },
          "title": "Arguments",
          "type": "array"
        },
        "metadata": {
          "title": "Metadata",
          "type": "object"
        },
        "path": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "format": "path",
              "type": "string"
            }
          ],
          "title": "Path"
        },
        "format": {
          "default": "text",
          "enum": [
            "text",
            "markdown",
            "jinja2"
          ],
          "title": "Format",
          "type": "string"
        },
        "type": {
          "const": "file",
          "default": "file",
          "title": "Type",
          "type": "string"
        },
        "watch": {
          "default": false,
          "title": "Watch",
          "type": "boolean"
        }
      },
      "required": [
        "name",
        "description",
        "path"
      ],
      "title": "FilePrompt",
      "type": "object"
    },
    "GlobalSettings": {
      "description": "Global settings that apply to all components.",
      "properties": {
        "timeout": {
          "default": 30,
          "title": "Timeout",
          "type": "integer"
        },
        "max_retries": {
          "default": 3,
          "title": "Max Retries",
          "type": "integer"
