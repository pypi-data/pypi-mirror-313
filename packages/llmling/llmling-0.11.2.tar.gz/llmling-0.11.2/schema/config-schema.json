        "timeout": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Timeout"
        }
      },
      "required": [
        "command"
      ],
      "title": "CLIResource",
      "type": "object"
    },
