{
    "wstl": {
        "title": "HEA Server AWS Storage Microservice",
        "actions": [
            {
                "name": "heaserver-storage-storage-get-open-choices",
                "description": "Open this storage",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "Open"
            },
            {
                "name": "heaserver-storage-storage-duplicate",
                "type": "safe",
                "target": "item",
                "prompt": "Duplicate"
            },
            {
                "name": "heaserver-storage-storage-open",
                "type": "safe",
                "target": "item",
                "prompt": "Open"
            },
            {
                "name": "heaserver-storage-storage-get-properties",
                "description": "View and edit this Awsstorage's properties",
                "type": "unsafe",
                "target": "item cj-template",
                "action": "update",
                "prompt": "Properties",
                "inputs": [
                    {
                        "name": "id",
                        "prompt": "Id",
                        "readOnly": true
                    },
                    {
                        "name": "display_name",
                        "prompt": "Name",
                        "required": true,
                        "readOnly": true
                    },
                    {
                        "name": "description",
                        "prompt": "Description",
                        "type": "textarea",
                        "readOnly": true
                    },
                    {
                        "name": "owner",
                        "prompt": "Owner",
                        "required": true,
                        "type": "select",
                        "hea": {
                            "optionsFromUrl": {
                                "path": "people/",
                                "value": "id",
                                "text": "display_name"
                            }
                        }
                    },
                    {
                        "name": "created",
                        "prompt": "Created",
                        "readOnly": true,
                        "hea": {
                            "type": "datetime"
                        }
                    },
                    {
                        "name": "modified",
                        "prompt": "Modified",
                        "readOnly": true,
                        "hea": {
                            "type": "datetime"
                        }
                    },
                    {
                        "name": "source",
                        "prompt": "Source",
                        "readOnly": true
                    },
                     {
                        "hea": {
                            "section": "shares",
                            "sectionPrompt": "Shares",
                            "optionsFromUrl": {
                                "path": "people/",
                                "value": "id",
                                "text": "display_name"
                            }
                        },
                        "name": "user",
                        "prompt": "User",
                        "required": true,
                        "type": "select"
                    },
                    {
                        "hea": {
                            "section": "shares",
                            "cardinality": "multiple"
                        },
                        "name": "permissions",
                        "prompt": "Permissions",
                        "type": "select",
                        "suggest": [
                            {"value":  "COOWNER", "text":  "Co-owner"},
                            {"value":  "CREATOR", "text":  "Creator"},
                            {"value":  "DELETER", "text":  "Deleter"},
                            {"value":  "EDITOR", "text":  "Editor"},
                            {"value":  "SHARER", "text":  "Sharer"},
                            {"value":  "VIEWER", "text":  "Viewer"}
                        ]
                    }
                ]
            },
            {
                "name": "heaserver-storage-storage-duplicate-form",
                "description": "Duplicate this awsstorage",
                "type": "unsafe",
                "target": "item cj-template",
                "action": "update",
                "prompt": "Duplicate",
                "inputs": [
                    {
                        "name": "id",
                        "prompt": "Id",
                        "required": true,
                        "readOnly": true
                    },
                    {
                        "name": "source",
                        "prompt": "Source",
                        "readOnly": true
                    },
                    {
                        "name": "version",
                        "prompt": "Version",
                        "readOnly": true
                    },
                    {
                        "name": "display_name",
                        "prompt": "Name",
                        "required": true,
                        "readOnly": true
                    },
                    {
                        "name": "description",
                        "prompt": "Description",
                        "type": "textarea",
                        "readOnly": true
                    },
                    {
                        "name": "owner",
                        "prompt": "Owner",
                        "readOnly": true
                    },
                    {
                        "name": "created",
                        "prompt": "Created",
                        "readOnly": true
                    },
                    {
                        "name": "modified",
                        "prompt": "Modified",
                        "readOnly": true
                    },
                    {
                        "name": "invited",
                        "prompt": "Share invited",
                        "readOnly": true
                    },
                    {
                        "name": "shared_with",
                        "prompt": "Shared with",
                        "readOnly": true
                    },
                    {
                        "name": "derived_by",
                        "prompt": "Derived by",
                        "readOnly": true
                    },
                    {
                        "name": "derived_from",
                        "prompt": "Derived from",
                        "readOnly": true
                    }
                ]
            }
        ]
    }
}
