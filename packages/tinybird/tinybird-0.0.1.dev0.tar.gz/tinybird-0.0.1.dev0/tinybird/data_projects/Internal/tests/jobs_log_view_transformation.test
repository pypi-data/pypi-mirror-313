tb sql "SELECT job_id, argMax(job_type, last_updated_at) as job_type, argMax(status, last_updated_at) as status, argMax(error, last_updated_at) as error, workspace_id, argMax(request_id, last_updated_at) as request_id, argMax(pipe_id, last_updated_at) as pipe_id, argMax(datasource_id, last_updated_at) as datasource_id, created_at, argMax(started_at, last_updated_at) as started_at, max(last_updated_at) as updated_at, argMax(job_metadata, last_updated_at) as job_metadata FROM jobs_log WHERE workspace_id='7d74a529-3e1d-4f77-b54b-61cd0c1635d2' GROUP BY workspace_id, created_at, job_id ORDER BY job_id DESC" --format csv
