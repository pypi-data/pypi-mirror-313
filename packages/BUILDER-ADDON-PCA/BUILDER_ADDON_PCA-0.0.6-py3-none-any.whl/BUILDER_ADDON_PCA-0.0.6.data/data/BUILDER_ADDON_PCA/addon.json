{
  "identifier": "com.seeq.pca",
  "name": "PCA",
  "description": "This addon let you perform Principal Component Analysis over data on your workbench to detect anomalies and root cause analysis over time-capsules defined as Test Periods, starting from a reference of a specific Train Period",
  "version": "0.0.1",
  "maintainer": "e-matica",
  "license": "GNU General Public License",
  "icon": "fa-bullseye",
  "elements": [
    
    {"name": "PCA",
      "identifier": "com.seeq.pca.addontool",
      "description": "Perform Principal Component Analysis over data of your workbench, starting from Training Period and a Test Period",
      "type": "AddOnTool",
      "path": "pca-addon-tool",
      "notebook_file_path": "Principal Component Analysis.ipynb",
      "configuration_schema": {
        "type": "object",
        "properties": {
          "display": {
            "type": "object",
            "properties": {
              "icon": {
                "type": "string",
                "default": "fa-bullseye"
              },
              "launchLocation": {
                "enum": ["toolsPane", "homescreen"],
                "default": "toolsPane"
              },
              "linkType": {
                "enum": ["window", "tab", "none"],
                "default": "tab"
              },
              "sortKey": {
                "type": "string",
                "default": "S"
              },
              "windowDetails": {
                "type": "string",
                "default": "toolbar=0,location=0,scroll,height=800,width=1000"
              }
            },
            "required": [
              "icon",
              "linkType",
              "sortKey",
              "windowDetails"
            ]
          },
          "project": {
            "type": "object",
            
            "required": []
          },
          "advanced_project_configuration": {
            "type": "object",
            "properties": {
              "kernel_name": {
                "type": "string",
                "default": "python311"
              }
            },
            "required": ["kernel_name"]
          }
        },
        "required": [
          "display",
          "project",
          "advanced_project_configuration"
        ]
      },
      "configuration_filename": "config",
      "configuration_converter": "json"
    }
    
    
    
    
    
  ],
  "previews": [],
  "tags": {}
}
